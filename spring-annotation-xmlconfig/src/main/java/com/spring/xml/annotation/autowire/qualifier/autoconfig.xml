<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"

	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://www.springframework.org/schema/context
        http://www.springframework.org/schema/context/spring-context.xsd">

	<context:annotation-config></context:annotation-config>

	<bean class="com.spring.xml.annotation.autowire.qualifier.Address"
		name="address1">
		<property name="street" value="Rajiv Nagar" />
		<property name="state" value="maharashtra" />
	</bean>
	
	<bean class="com.spring.xml.annotation.autowire.qualifier.Address"
		name="address2">
		<property name="street" value="friends colony" />
		<property name="state" value="maharashtra" />
	</bean>

	<bean class="com.spring.xml.annotation.autowire.qualifier.Student"
		name="student">
		<property name="id" value="12345" />
		<property name="name" value="pasha" />
		<property name="city" value="nashik" />
	</bean>


</beans>
